    "linear_pipeline = Pipeline([('poly', PolynomialFeatures(degree=3)), \n",
    "                            ('model', LinearRegression())])\n",
    "\n",
    "ridge_pipeline = Pipeline([('poly', PolynomialFeatures(degree=3)), \n",
