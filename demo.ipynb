    "                            ('model', TheilSenRegressor(fit_intercept=True))])\n",
    "\n",
    "\n",
    "# Combine them into a StackingRegressor\n",
    "stacking_regressor = StackingRegressor(\n",
    "    estimators=[\n",
    "        ('linear', linear_pipeline),\n",
    "        ('ridge', ridge_pipeline),\n",
    "        ('lasso', lasso_pipeline),\n",
